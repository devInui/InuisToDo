{
  "name": "InuisToDo",
  "description": "This is a ToDo app for inui. It's currently under development.",
  "version": "0.1.0",
  "license": "",
  "author": {
    "name": "devInui"
  },
  "repository": {
    "url": "https://github.com/p6b39/my-electron-app.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:webpack dev:electron dev:react-devtools",
    "dev:react-devtools": "react-devtools",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on dist/main.js dist/index.html && electronmon ."
  },
  "electronmon": {
    "patterns": [
      "dist/**/*"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.2",
    "electron-store": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^25.3.0",
    "electronmon": "^2.0.2",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.3",
    "react-devtools": "^4.28.0",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.3",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  }
}
